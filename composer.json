{
    "name": "tea/collections",
    "description": "A utility library for working with collections and arrays.",
    "keywords": [
        "collection", "arrays", "ArrayAccess", "iterable", "Traversable",
        "utility", "helpers", "utils", "tea"
    ],
    "license": "MIT",
    "homepage": "https://github.com/TeaLabs/collections",
    "support": {
        "issues": "https://github.com/TeaLabs/collections/issues",
        "source": "https://github.com/TeaLabs/collections"
    },
    "authors": [
        {
            "name": "David Kyalo",
            "email": "davidmkyalo@gmail.com",
            "homepage": "http://www.davidkyalo.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "tea/uzi": "~0.1.0",
        "tea/regex": "~0.1.0",
        "tea/future": "~0.1.0",
        "tea/contracts": "~0.1.0",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Tea\\Collections\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tea\\Collections\\Tests\\": "tests/"
        },
        "files": [
            "tests/functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
